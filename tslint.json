{
  "extends": "tslint-config-airbnb",
  "rules": {
    "max-line-length": [true, 200],
    "variable-name": false,
    "object-curly-spacing": false,
    "import-name": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "ter-arrow-parens": [true, "as-needed"],
    "one-variable-per-declaration": false,
    "align": false
  }
}